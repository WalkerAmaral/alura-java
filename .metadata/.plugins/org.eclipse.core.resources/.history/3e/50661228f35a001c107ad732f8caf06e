package exercicios;

public class SetimoExercicio {

	public static void main(String[] args) {
		
		
		int[] numero = {10, 25, 65, 20, 1, 3, 4, 8, 9, 3};
		
		for(int i = 0; i < numero.length; i++)  {
			
			if(numero[i] <= 10) {
				System.out.println(numero[i] + " é menor ou igual a 10");			
			} else {
				System.out.println(numero[i] + " é maior que 10");
			}

		}
		
		
	}
}
